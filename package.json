{
  "name": "contidio-widget",
  "version": "0.0.2",
  "description": "Include a single or multiple Contidio Assets or Overviews into your existing website",
  "repository": {
    "type": "git",
    "url": "https://github.com/contidio/contidio-widget.git"
  },
  "keywords": [
    "contidio",
    "javascript"
  ],
  "scripts": {
    "build": "browserify src/main.js | uglifyjs -c > dist/contidio-widget.min.js",
    "watch": "watchify src/main.js -o dist/contidio-widget.js --debug --verbose",
    "example": "http-server ./"
  },
  "dependencies": {
    "domtastic": "^0.14.0",
    "promise-polyfill": "^6.0.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "http-server": "^0.10.0",
    "uglifyify": "^3.0.4",
    "watchify": "^3.9.0"
  },
  "author": {
    "name": "Lukas Struber",
    "email": "ls@contidio.com",
    "url": "https://www.contidio.com"
  },
  "license": "MIT"
}
